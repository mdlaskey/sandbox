# Location of train/test prototxt file
net: "/Users/JonathanLee/Desktop/sandbox/vision/Net/trainer.prototxt"

test_iter: 5
test_interval: 5
#base_lr: 0.00001
base_lr: .001
lr_policy: "step"
gamma: 0.001
stepsize: 30
display: 1
max_iter: 300
# Lower momentum?
#momentum: 0.9
momentum: 0.9
weight_decay: 0.0005

# Number of iterations before snapshot of model is created
snapshot: 20
snapshot_prefix: "weights"
# Switch to GPU for GPU mode
solver_mode: CPU
